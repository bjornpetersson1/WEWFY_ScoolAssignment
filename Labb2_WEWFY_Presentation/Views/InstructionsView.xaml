<UserControl x:Class="Labb2_WEWFY_Presentation.Views.InstructionsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Labb2_WEWFY_Presentation.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="300" d:Background="White">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="4*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TextBlock Text="Testest"
                   HorizontalAlignment="Center"
                   Grid.Row="0" />
        <Button Content="OK"
                Command="{Binding MainVM.ChangeCurrentViewCommand}"
                CommandParameter="{Binding MainVM.MenuView}"
                Grid.Row="1"/>
    </Grid>
</UserControl>
